<div id="product-window" class="round_box box_shadow">
	<h1><%= @title %></h1>
	<table border="0" cellpadding="0" cellspacing="10" class="list">
		<% @product.each_with_index do |product, i| %>
			<tr <%= ' class="alt"' if i%2==0 %>>
				<td><a href="/show/<%= product.id %>"><%= product.title %></a></td>
				<td><a href="/delete/<%= product.id %>">delete</a></td>
		<% end %>
	</table>
</div>